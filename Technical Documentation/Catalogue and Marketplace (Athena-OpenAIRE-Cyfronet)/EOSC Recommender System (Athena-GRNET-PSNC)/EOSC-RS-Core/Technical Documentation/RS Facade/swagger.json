   "paths": {
      "/recommendations": {
         "post": {
            "tags": [
               "recommendation"
            ],
            "summary": "Returns a list of sets of recommendations for each of num_lists list.\n        Each of the set of recommendations contains 10 ids of recommended scientific services or resources,\n        explanations, and short explanations.",
            "description": "A function that generates personalized and anonymous recommendations\n    for the user based on the user context.",
            "operationId": "recommendations_recommendations_post",
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "$ref": "#/components/schemas/RecommendationContext"
                     }
                  }
               },
               "required": true
            },
            "responses": {
               "200": {
                  "description": "Successful Response",
                  "content": {
                     "application/json": {
                        "schema": {
                           "$ref": "#/components/schemas/RecommendationSet"
                        }
                     }
                  }
               },
               "422": {
                  "description": "Validation Error",
                  "content": {
                     "application/json": {
                        "schema": {
                           "$ref": "#/components/schemas/HTTPValidationError"
                        }
                     }
                  }
               }
            }
         }
      },
      "/project_completion/recommendation": {
         "post": {
            "tags": [
               "recommendation"
            ],
            "summary": "Get Project Completion Recommendation",
            "description": "**Suggest a completion for the project**\n\n        Based on the project given as input, we recommend services that are frequently combined\n        with the ones existing in the project.\n\n        - **project_id**: the id of the project currently viewed by the user\n        - **num**: number of recommendations we want returned\n\n        **Returns** a list of dicts where service_id is the id of the recommended service\n        and score is the support from the frequent item sets.",
            "operationId": "project_completion_recommendation_project_completion_recommendation_post",
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "$ref": "#/components/schemas/ProjectCompletionRecommendationParameters"
                     }
                  }
               },
               "required": true
            },
            "responses": {
               "200": {
                  "description": "Successful Response",
                  "content": {
                     "application/json": {
                        "schema": {
                           "items": {
                              "$ref": "#/components/schemas/ProjectCompletionRecommendation"
                           },
                           "type": "array",
                           "title": "Response Project Completion Recommendation Project Completion Recommendation Post"
                        }
                     }
                  }
               },
               "422": {
                  "description": "Validation Error",
                  "content": {
                     "application/json": {
                        "schema": {
                           "$ref": "#/components/schemas/HTTPValidationError"
                        }
                     }
                  }
               }
            }
         }
      },
      "/similar_services/recommendation": {
         "post": {
            "tags": [
               "recommendation"
            ],
            "summary": "Get Similar Services Recommendation",
            "description": "**Suggest a similar service**\n\n        Based on the service given as input, we recommend similar services utilizing both textual\n        and metadata attributes.\n\n        - **user_id**: the id of the user (as it was given in the marketplace)\n        - **unique_id**: the unique identifier of the not logged in user \n        - **aai_uid**: the unique AAI identifier of the logged user\n        - **timestamp** the exact time of the recommendation request sending\n        - **service_id**: the id of the service currently viewed by the user\n        - **num**: number of recommendations we want returned\n\n        **Returns** a collection of recommended services.",
            "operationId": "similar_services_recommendation_similar_services_recommendation_post",
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "$ref": "#/components/schemas/SimilarServicesRecommendationParameters"
                     }
                  }
               },
               "required": true
            },
            "responses": {
               "200": {
                  "description": "Successful Response",
                  "content": {
                     "application/json": {
                        "schema": {
                           "$ref": "#/components/schemas/RecommendationSet"
                        }
                     }
                  }
               },
               "422": {
                  "description": "Validation Error",
                  "content": {
                     "application/json": {
                        "schema": {
                           "$ref": "#/components/schemas/HTTPValidationError"
                        }
                     }
                  }
               }
            }
         }
      },
